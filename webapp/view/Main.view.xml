<mvc:View xmlns:launchpad="sap.ushell.ui.launchpad"
	controllerName="sapbtpproject.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	core:require="{
		formatter: 'sapbtpproject/model/formatter'
	}">

 <Page id="mainPage" >
 <headerContent>
 <Bar >
 <contentLeft>
<Image src="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg" width="60px" height="2rem" />
 <Link id="productsPage" text="Products"></Link>
 <Link id="supportPage" text="Support"></Link>
 <Link id="learningPage" text="Learning"></Link>
 <Link id="communityPage" text="Community"></Link>
 <Link id="othersPage" text="Partners"></Link>
 <Link id="aboutPage" text="About"></Link>
 </contentLeft>
 <contentRight>
 <Text text="Explore SAP"/>
 <Button icon="sap-icon://search"/>
 <Button icon="sap-icon://person-placeholder"/>
 <Button icon="sap-icon://world"/>
 </contentRight>
 </Bar>
 </headerContent>
 </Page>

</mvc:View>
